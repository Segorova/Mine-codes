function roots(a,b,c){
  let discr = Math.pow(b, 2) - 4 * a * c;
  let x1 , x2;
  
  if(discr === 0) {
    x1 = -b / (2 * a);
    x1 = x2;
   }
  if(discr > 0) {
    x1 = (-b + Math.sqrt(discr)) / (2 * a);
    x2 = (-b - Math.sqrt(discr)) / (2 * a);
   }
  else return null;
    console.log(-b);
  return +(x1 + x2).toFixed(2);

}
