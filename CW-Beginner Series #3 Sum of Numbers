function getSum( a,b ){
  let min = Math.min(a,b), max = (min === a ? b : a), arr= []
  for (let i = min; i <= max; i++){
    arr.push(i)
  }
 return arr.reduce((acc,el)=> acc+el)
}

*********************************

function GetSum(a, b) {
   return a > b ? GetSum(b, a) : (b - a + 1) * (b + a) / 2;
}

***********************************

GetSum = (a, b) => (a + b) * (Math.abs(a - b) + 1) / 2;

*********************************************

