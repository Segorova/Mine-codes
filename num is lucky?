function isLucky(n) {
  n = String(n);
  let sum = 0;
    
  for(let i = 0; i < (n.length); i++) {
      sum += +n[i];
      }
 console.log(sum);
  if(sum % 9 === 0 || sum === 0) return true;
  else return false;
}
console.log(isLucky(1892376));

другое решение :
 
function isLucky(n) {
  return n % 9 == 0;
}
console.log(isLucky(1892376));
