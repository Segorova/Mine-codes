function alphabetWar(fight){
  let res = 0
  let letters = {
     w : 4, p : 3, b : 2, s : 1,
     m : -4, q : -3, d : -2, z : -1,
  }
  for (let i = 0; i < fight.length; i++){
    for (let key in letters){
      if (fight[i] === key) res += letters[key]     
    }} 
   return (res === 0) ? "Let's fight again!": (res > 0)? "Left side wins!":"Right side wins!"
}


