function sumMix(x){
return x.map(el => +el).reduce((acc,el) => acc + el)
}
*************************
function sumMix(x){
  let result = 0;
  for (let n of x) {
    result += parseInt(n);
  }
  return result;
}

console.log(sumMix([1, '5', 2,'8', 3,'9', 4]));
console.log(sumMix([17, '56', 23,'82', 31,'95', 49]));
