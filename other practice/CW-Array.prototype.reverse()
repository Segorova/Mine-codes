Array.prototype.reverse = function() {
 for (let i = 0; i < this.length / 2; i++) {
  let temp = this[i];
  this[i] = this[this.length - 1 - i];
  this[this.length - 1 - i] = temp;
 }
    return this
};

*******************************
Array.prototype.reverse = function() {
  var arr = this.splice(0);  
  
  while(arr.length) {
    this.push(arr.pop());
  }   
  
  return this;
};
*************************************
Array.prototype.reverse = function() {
  for(var i = 0, j = this.length-1; i < j; i++, j--) {
    var tmp = this[i];
    this[i] = this[j];
    this[j] = tmp;
  }
  return this;
};
