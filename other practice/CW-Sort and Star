function twoSort(s) {
let arr = s.sort()
return arr[0].split('').join('***')
}

*************************************************

function twoSort(s) {
  return s.sort()[0].split('').join('***');
}

******************************************

twoSort = s => s.sort()[0].split('').join('***')

*******************************************
const twoSort = s => [...s.sort()[0]].join('***');
