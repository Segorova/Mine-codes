Array.prototype.only = function(keys){
  let arr = []
  keys = keys.sort((a,b) => a-b)
    for (let i = 0; i < keys.length; i++) {
      arr.push(this[keys[i]])
    }
  return arr
}
***************************************

Array.prototype.only = function(k) {
  return this.filter((e, i) => k.includes(i));
}
***************************************
