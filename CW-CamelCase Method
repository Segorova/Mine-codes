String.prototype.camelCase=function(){
 let s = this.trimLeft().trimRight()
  return this.length > 0 ? s.split(' ').map(el => el[0].toUpperCase()+ el.slice(1)).join('') : ''
}
*************************************
String.prototype.camelCase=function(){
  return this.split(' ').map(w => w.slice(0, 1).toUpperCase() + w.slice(1)).join('');
}
***************************************
